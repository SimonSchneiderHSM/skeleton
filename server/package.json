  "publishConfig": {
    "registry": "https://www.npmjs.com"
  },
  "version": "0.1.0",
  "description": "demonstration project for showcasing the process engine",
  "license": "MIT",
  "main": "dist/commonjs/index.js",
  "typings": "dist/index.d.ts",
  "contributors": [
    "Sebastian Meier <sebastian.meier@5minds.de>",
    "Patrick PÃ¶tschke <patrick.poetschke@quantusflow.com>",
